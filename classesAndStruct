import Foundation

// Classes and Structs


class Car {
    
    let make: String
    let color: String
    
    init(make: String, color: String) {
        self.make = make
        self.color = color
    }
}

var bmw: Car = Car(color: "blue", make: "bmw")
var car2 = bmw

// car2 -> bmw
// car2 | bmw
